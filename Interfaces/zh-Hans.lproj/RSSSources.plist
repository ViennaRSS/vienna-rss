<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>本地文件</key>
	<dict>
		<key>LinkName</key>
		<string>输入你的用户文件夹路径</string>
		<key>LinkTemplate</key>
		<string>file://~/%@</string>
	</dict>
	<key>LiveJournal</key>
	<dict>
		<key>SiteHomePage</key>
		<string>http://www.livejournal.com</string>
		<key>LinkName</key>
		<string>输入 LiveJournal 用户名</string>
		<key>LinkTemplate</key>
		<string>http://www.livejournal.com/users/%@/data/rss</string>
	</dict>
	<key>MSN Spaces</key>
	<dict>
		<key>SiteHomePage</key>
		<string>http://spaces.msn.com</string>
		<key>LinkName</key>
		<string>输入 MSN Spaces 用户名</string>
		<key>LinkTemplate</key>
		<string>http://spaces.msn.com/members/%@/feed.rss</string>
	</dict>
	<key>Blogspot</key>
	<dict>
		<key>SiteHomePage</key>
		<string>http://www.blogspot.com</string>
		<key>LinkName</key>
		<string>输入 Blogspot 用户名</string>
		<key>LinkTemplate</key>
		<string>http://%@.blogspot.com/atom.xml</string>
	</dict>
	<key>Xanga</key>
	<dict>
		<key>SiteHomePage</key>
		<string>http://www.xanga.com</string>
		<key>LinkName</key>
		<string>输入 Xanga 用户名</string>
		<key>LinkTemplate</key>
		<string>http://www.xanga.com/rss.aspx?user=%@</string>
	</dict>
	<key>URL</key>
	<dict>
		<key>LinkName</key>
		<string>输入 RSS 频道的 URL 地址</string>
		<key>LinkTemplate</key>
		<string>%@</string>
	</dict>
</dict>
</plist>
