<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Local File</key>
	<dict>
		<key>LinkName</key>
		<string>Enter the path under your home folder</string>
		<key>LinkTemplate</key>
		<string>file://~/%@</string>
	</dict>
	<key>LiveJournal</key>
	<dict>
		<key>SiteHomePage</key>
		<string>http://www.livejournal.com</string>
		<key>LinkName</key>
		<string>Enter LiveJournal User name</string>
		<key>LinkTemplate</key>
		<string>http://www.livejournal.com/users/%@/data/rss</string>
	</dict>
	<key>Blogspot</key>
	<dict>
		<key>SiteHomePage</key>
		<string>http://www.blogspot.com</string>
		<key>LinkName</key>
		<string>Enter Blogspot User name</string>
		<key>LinkTemplate</key>
		<string>http://%@.blogspot.com/atom.xml</string>
	</dict>
	<key>URL</key>
	<dict>
		<key>LinkName</key>
		<string>Enter URL of RSS feed</string>
		<key>LinkTemplate</key>
		<string>%@</string>
	</dict>
</dict>
</plist>
